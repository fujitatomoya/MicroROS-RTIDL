    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/uros-rtidl",
            "args": [
                "-baseType",
                "Twist",
                "-typeDir",
                "${workspaceFolder}/test/msg",
                "-outDir",
                "${workspaceFolder}/test/output",
                "-pyTemplate",
                "./test/pythonTypeTemplate.j2"

            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "MIMode": "gdb",
            "externalConsole": false,
            "setupCommands": [
            ] 
        }
    ]
}